@model IEnumerable<GamesShop.Models.Orders>

<h3>История заказов</h3>
<div>
   <table class="table table-bordered table-striped">
       <tr>
           <th scope="col">Id заказа</th>
           <th scope="col"> Дата заказа:</th>
           <th scope="col">Цена заказа</th>
           <th scope="col"></th>
       </tr> 
       @foreach(var order in Model){
       <tr>
           <td>@order.IdOrder</td>
           <td>@order.DateOfZakaz</td>
           <td>@order.Price</td>
           <td><a asp-controller="Order" asp-action="DetailOrderOfUser" asp-route-id="@order.IdOrder">Посмотреть детали</a></td>
       </tr>
       }
   </table>
</div>