<configuration>
  <system.webServer>
    <security>
      <requestFiltering>
        <requestLimits maxQueryString="3000" maxUrl="1000" />
      </requestFiltering>
    </security>
  </system.webServer>
  <system.web>
    <httpRuntime maxQueryStringLength="32768" maxUrlLength="65536"/>
  </system.web>
</configuration>